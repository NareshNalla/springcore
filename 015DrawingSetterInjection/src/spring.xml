<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="triangle" class="com.sekharit.spring.Triangle">
		<property name="pointOne" ref="p1"></property>
		<property name="pointTwo" ref="p2"></property>
		<property name="pointThree" ref="p3"></property>
	</bean>

	<bean id="circle" class="com.sekharit.spring.Circle">
		<property name="center" ref="p4"></property>
	</bean>

	<bean id="p1" class="com.sekharit.spring.Point">
		<property name="x" value="10"></property>
		<property name="y" value="20"></property>
	</bean>

	<bean id="p2" class="com.sekharit.spring.Point">
		<property name="x" value="15"></property>
		<property name="y" value="25"></property>
	</bean>

	<bean id="p3" class="com.sekharit.spring.Point">
		<property name="x" value="30"></property>
		<property name="y" value="40"></property>
	</bean>

	<bean id="p4" class="com.sekharit.spring.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>

</beans>
