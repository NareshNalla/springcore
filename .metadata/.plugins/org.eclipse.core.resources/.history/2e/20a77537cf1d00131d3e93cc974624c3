package com.vidvaan.bean;

public class Employee {
	private int eno;
	private String ename;
	private double salary;

	public Employee() {
		System.out.println("Employee()");
	}

	public Employee(int eno, String ename, double salary) {
		System.out.println("Employee(int eno, String ename, double salary)");
		this.eno = eno;
		this.ename = ename;
		this.salary = salary;
	}

	public int getEno() {
		return eno;
	}

	public void setEno(int eno) {
		System.out.println("setEno(int eno)");
		this.eno = eno;
	}

	public String getEname() {
		return ename;
	}

	public void setEname(String ename) {
		System.out.println("setEname(String ename)");
		this.ename = ename;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		System.out.println("setSalary(double salary)");
		this.salary = salary;
	}
	

}
