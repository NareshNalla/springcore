package com.vidvaan.demo;

import org.springframework.beans.factory.BeanFactory;
import org.springframework.beans.factory.xml.XmlBeanFactory;
import org.springframework.core.io.ClassPathResource;

import com.vidvaan.bean.Employee;

public class InjectionTest {
	public static void main(String[] args) {
	
		BeanFactory beanFactory = new XmlBeanFactory(new ClassPathResource("com/vidvaan/config/spring.xml"));
		Employee employee = (Employee)beanFactory.getBean("emp");
		System.out.println("eno : "+employee.getEno());
		System.out.println("ename : "+employee.getEname());
		System.out.println("salary : "+employee.getSalary());
		
	}
}
