package com.vidvaan.bean;

public class Employee {
	private int eno;
	private String ename;
	private double salary;
	
	public Employee() {
		System.out.println("Employee()");
	}

	public Employee(int eno, String ename, double salary) {
		this.eno = eno;
		this.ename = ename;
		this.salary = salary;
	}
	
	
}
