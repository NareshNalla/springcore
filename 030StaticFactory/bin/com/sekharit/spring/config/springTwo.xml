<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean 	id="springBeanId" 
			class="com.sekharit.spring.bean.SpringBeanTwo" 
			factory-method="newInstance" >
		<constructor-arg value="Sekhar" />
	</bean>
	<!--  Here spring container creates the object of SpringBeanTwo, 
		by  calling newInstance(String arg) static factory method. -->  


</beans>
